{
  "name": "@mioto/editor",
  "version": "1.1.9",
  "type": "module",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "proxy": "caddy reverse-proxy --from localhost:4300 --to :4200",
    "validate-env": "tsx ../../scripts/validate-env.ts",
    "build": "tsx ./scripts/build.ts",
    "build:dev": "pnpm env:dev -- pnpm build",
    "serve": "NODE_OPTIONS='--inspect' pnpm env:dev -- pnpm exec next dev -p 4200 --turbo",
    "lint": "pnpm biome check --apply ./",
    "lint:unsafe": "pnpm biome check --apply-unsafe ./",
    "ignore": "pnpm dlx turbo-ignore @mioto/editor --task=build",
    "env:dev": "dotenvx run -f .env.development",
    "docker:build": "APP_PATH='apps/editor' pnpm -w env:prod -- bash ./scripts/docker-build.sh",
    "prepare:docker:build": "rm -rf ./../../out && turbo prune @mioto/editor --docker"
  },
  "dependencies": {
    "@ariakit/react": "^0.4.18",
    "@aws-sdk/client-s3": "3.750.0",
    "@hookform/error-message": "^2.0.1",
    "@langchain/community": "^0.3.55",
    "@langchain/core": "^0.3.75",
    "@langchain/openai": "^0.4.9",
    "@mdx-js/loader": "^3.1.1",
    "@mioto/analytics": "workspace:^",
    "@mioto/design-system": "workspace:^",
    "@mioto/email": "workspace:^",
    "@mioto/env": "workspace:^",
    "@mioto/errors": "workspace:^",
    "@mioto/locale": "workspace:^",
    "@mioto/prisma": "workspace:^",
    "@mioto/server": "workspace:^",
    "@mioto/workflow-builder": "workspace:^",
    "@next/mdx": "15.1.7",
    "@opentelemetry/instrumentation": "^0.57.2",
    "@phosphor-icons/react": "^2.1.10",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/nextjs": "^9.46.0",
    "@sentry/utils": "^8.55.0",
    "@tailwindcss/postcss": "^4.1.13",
    "@tanstack/react-query": "^5.87.1",
    "@xstate/react": "^5.0.5",
    "angular-expressions": "^1.5.1",
    "argon2": "^0.41.1",
    "autoprefixer": "^10.4.21",
    "chromiumly": "^3.11.0",
    "date-fns": "4.1.0",
    "framer-motion": "12.4.7",
    "langchain": "^0.3.33",
    "lodash": "^4.17.21",
    "match-sorter": "^8.1.0",
    "next": "15.5.7",
    "next-intl": "3.26.5",
    "nextjs-toploader": "^3.9.16",
    "pizzip": "^3.2.0",
    "posthog-js": "^1.262.0",
    "react": "19.1.0",
    "react-aria-components": "^1.12.1",
    "react-dom": "19.1.0",
    "react-pdf": "^9.2.1",
    "react-use": "^17.6.0",
    "remeda": "2.20.2",
    "sharp": "^0.33.5",
    "styled-jsx": "^5.1.7",
    "ts-pattern": "^5.8.0",
    "tslib": "^2.8.1",
    "validator": "^13.15.15",
    "valtio": "^2.1.7",
    "xstate": "^5.21.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/office-js": "1.0.471",
    "@types/office-runtime": "^1.0.35",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/validator": "^13.15.3",
    "tailwindcss": "^4.1.13",
    "utility-types": "^3.11.0"
  }
}